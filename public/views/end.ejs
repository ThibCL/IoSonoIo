<!DOCTYPE html>
<html>

<head>
    <link rel="stylesheet" type="text/css" href="/style/end.css" />
    <script src="https://kit.fontawesome.com/8832483f9d.js" crossorigin="anonymous"></script>
    <script src="/js/end.js"></script>

</head>



<body>

    <div class="block screen">
        <!-- logo -->
        <div id="logo-container">
            <div id="logo">io sono io</div>
        </div>
        <div id="avatar-container">
            <!-- get avatar information from the server -->
            <img id="avimg" src="<%=history.skin_tone.general.url%>">
        </div>
    </div>
    <div class="block history">
        <div class="title-container" style="padding: 40px;">
            <p class="title">History</p>
        </div>

        <div id="lines" style="height: 70vh; overflow: auto; padding-left: 40px; padding-bottom:40px">
        </div>

        <div class="end_form">
            <form class="end_form" method="POST" action="/game/<%=id%>/endgame">
                <input id="end" type="submit" value="end">
            </form>
        </div>

    </div>





    </div>
    </div>
    <script type="text/javascript">
        var history = startHistory('<%- JSON.stringify(history) %>')
    </script>
    <script src="/js/history.js"></script>
</body>

</html>